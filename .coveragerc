#[run]
#branch = True

[report]
exclude_line =
	pragma: no cover
	def __repr__
	if self\.debug
	raise AssertionError
	raise NotImplementedError
	if 0:
	if False:
	if __name__ == .__main__.:
	pass

ignore_errors = True

[run]
omit =
	*/__init__.py
	/qa/bin/*
	/qa/sbin/*
	/qa/conf/*
	/dev/*
	/etc/*
	/bin/*
	/sbin/*
	/dist/*
	/debian/*
	/service/*

#[path]
#source =
